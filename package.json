{
	"name": "tars",
	"displayName": "tars",
	"description": "Syntax highlighting for tars",
	"version": "0.0.1",
	"icon": "images/icon.png",
	"publisher": "fidelchen",
	"repository": {
		"type": "git",
		"url": "https://github.com/fidelchen/tfi-tars.git"
	},
	"engines": {
		"vscode": "^1.16.0"
	},
	"categories": [
		"Languages",
		"Snippets"
	],
	"contributes": {
		"languages": [
			{
				"id": "tars",
				"aliases": [
					"tars",
					"TARS"
				],
				"extensions": [
					".tars"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "tars",
				"scopeName": "source.tars",
				"path": "./syntaxes/tars.tmLanguage"
			}
		],
		"snippets": [
			{
				"language": "tars",
				"path": "./snippets/tars.json"
			}
		]
	}
}
